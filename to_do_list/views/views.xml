<odoo>
  <data>
    <record id="to_do_list_tree_view" model="ir.ui.view">
      <field name="name">to_do_list.titit.view.tree</field>
      <field name="model">to_do_list.titit</field>
      <field name="arch" type="xml">
        <tree string="Judul">
          <field name="check" />
          <field name="title" />
          <field name="description" />
        </tree>
      </field>
    </record>
    <record id="to_do_list_action" model="ir.actions.act_window">
      <field name="name">to_do_list</field>
      <field name="res_model">to_do_list.titit</field>
      <field name="view_type">form</field>
      <field name="view_mode">tree,form</field>
    </record>
    <menuitem
      id="to_do_list_menu"
      name="to_do_list"
      sequence="10"/>
      <menuitem
        id="task_menu"
        name="task"
        action="to_do_list_action"
        parent="to_do_list_menu"
        sequence="20"/>
  </data>
</odoo>